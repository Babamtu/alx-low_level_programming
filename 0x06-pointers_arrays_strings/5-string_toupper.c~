#include "main.h"

/**
 * string_toupper - changes all lowercase letters to capital
 * @str: string to be modified
 *
 * Return: char val
 */

char *string_toupper(char *)
{
	int i = 0;

	while (str[i] != '\0')
	{
		if (str[i] >= 'a' && str[i] <= 'z')
		{
			str[i] -= 32;
		}
		i++;
	}
	return (str);
}
